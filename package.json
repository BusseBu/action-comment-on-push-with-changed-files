{
  "name": "action-comment-on-push-with-changed-files",
  "version": "1.0.0",
  "private": true,
  "description": "This is a github action to post a reply to comments in PRs if file to which the comment was left has been changed",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BusseBu/action-comment-on-push-with-changed-files"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "BusseBu",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "^2.1.1",
    "@octokit/webhooks": "^7.1.2"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^12.7.12",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^23.8.2",
    "jest": "^25.2.7",
    "jest-circus": "^25.2.7",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^25.3.1",
    "typescript": "^3.6.4"
  }
}
